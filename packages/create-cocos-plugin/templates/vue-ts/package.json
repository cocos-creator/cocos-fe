{
    "name": "cocos-plugin-vue3",
    "private": true,
    "version": "0.0.1",
    "package_version": 2,
    "type": "module",
    "main": "./dist/browser.cjs",
    "scripts": {
        "dev": "npm run build -- --mode development",
        "build": "vue-tsc -b && vite build"
    },
    "devDependencies": {
        "@cocos/creator-types": "^3.8.3",
        "@vitejs/plugin-vue": "^5.1.2",
        "rollup-plugin-node-externals": "^7.1.3",
        "typescript": "^5.5.3",
        "vite": "^5.4.1",
        "vue-tsc": "^2.0.29"
    },
    "dependencies": {
        "vue": "^3.4.37"
    },
    "contributions": {
        "menu": [
            {
                "path": "i18n:menu.panel",
                "label": "打开 vue3 面板",
                "message": "open"
            }
        ],
        "messages": {
            "open": {
                "methods": [
                    "open"
                ]
            }
        }
    },
    "panels": {
        "default": {
            "main": "./dist/panel_default.cjs",
            "size": {
                "min-width": 400,
                "min-height": 300,
                "width": 600,
                "height": 500
            }
        }
    }
}
