    "private": true,
    "version": "0.0.1",
    "package_version": 2,
    "type": "commonjs",
    "main": "./dist/browser.js",
    "scripts": {
        "dev": "npx hi-cocos dev .",
        "build": "vue-tsc && npx hi-cocos build ."
    },
    "devDependencies": {
        "@vitejs/plugin-vue2": "^2.3.1",
        "rollup-plugin-node-externals": "^7.1.3",
        "typescript": "^5.4.3",
        "vue-tsc": "^2.0.7"
    },
    "dependencies": {
        "vue": "^2.7.16"
    },
    "contributions": {
        "menu": [
            {
                "path": "i18n:menu.panel",
                "label": "打开vue2面板",
                "message": "open"
            }
